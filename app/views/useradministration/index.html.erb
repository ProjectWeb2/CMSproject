<p id="notice"><%= notice %></p>

<h1>User</h1>

<table>
  <thead>
  <tr>
    <th>id</th>
    <th>email</th>
    <th colspan="3"></th>
  </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
          <tr>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= link_to image_tag('edit.png', height: '20', width: '20'), useradministration_path(:id => user.id)  %></td>

            <td><%= link_to image_tag('delete.png', height: '20', width: '20'),  useradministration_path(user), data: { confirm: "Are you sure?" }, method: :delete  %></td>
          </tr>
  <% end %>
  </tbody>
</table>


<br>

