<h1>New Content</h1>

<%= semantic_form_for @content do |f| %>
        <%= f.inputs do %>
                <%= f.input :featured_image, :as => :rich_picker %>
                <%= f.input :text, :as => :rich %>
        <% end %>
        <%= f.buttons do %>
                <%= f.commit_button %>
        <% end %>
<% end %>

<%= link_to 'Back', contents_path %>
